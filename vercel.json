{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/",
            "runtime":"@vercel/node"
        }
    ],
    "functions": {
        "*": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}