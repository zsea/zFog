{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "*": {
            "memory": 1024,
            "maxDuration": 10,
            "runtime":"@vercel/node"
        }
    }
}